<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:jaxws="http://cxf.apache.org/jaxws"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
                       http://cxf.apache.org/jaxws   http://cxf.apache.org/schemas/jaxws.xsd">

	<import resource="classpath:META-INF/cxf/cxf.xml" />
	<import resource="classpath:META-INF/cxf/cxf-servlet.xml" />

	<!-- id : Any name -->
	<!-- implementor : Name of the class which implements the service interface -->
	<!-- address : path name relative to the application context. This will 
		resolve into http://localhost:8080/cxf-service-one/MathModule -->
	<jaxws:endpoint id="mathModule"
		implementor="com.codereq.cxfservices.MathModuleImpl" address="/MathModule" />
	
</beans>
